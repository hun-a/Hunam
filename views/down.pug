doctype html
html(lang='ko')
  head
    title Down page
    link(rel='stylesheet' type='text/css' media='all' href='/css/audio.css')
    script(src='https://code.jquery.com/jquery-3.2.1.min.js')
    script(type='text/javascript' src='/js/login.js')
  body
    div(class='container')
      div(class='column center')
        h1 The list of Downloaded files
        br
        table
          each music in musics
            tr
              td
                image(src=music.thumbnail height='150' width='220')
              td
                = music.title
                p
                  = music.duration
                p
                  button(class='select-music' onClick='add(' + music.key + ')') add
